/**
 * This class does as it should 
 * so that way the first parts for the
 * results per candidate can print properly.
 * 
 * @author Annabel Hartmann
 * @version 3/5/18
 */

public class Candidate {
	
	public String name;
	public int votes;
	
	public Candidate(String n, int v) {
		this.name = n;
		this.votes = v;
    }
	
    public String getName() {
	return name;
	}
    
	public int getVotes() {
	return votes;
	}
	
    public String toString() {
        return getName() + " received " + getVotes() + " votes. ";
    }
}
